{
	"name": "srggroup/zf2nocaptcha",
	"description": "Google NoCaptcha for Laminas",
	"type": "library",
	"keywords": [
		"zf2",
		"recaptcha",
		"nocaptcha"
	],
	"homepage": "http://srg.hu/",
	"license": "BSD-3-Clause",
	"autoload": {
		"psr-0": {
			"NoCaptcha\\": "library/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.srggroup.dev/"
		}
	],
	"require": {
		"php": "^8.3 | ^8.4",
		"google/recaptcha": "^1.3.1",
		"laminas/laminas-http": "^2.22.0",
		"laminas/laminas-captcha": "^2.13.0",
		"laminas/laminas-form": "^3.21.0"
	},
	"require-dev": {
		"srggroup/srg_standards": "dev-master"
	},
	"extra": {
		"phpcodesniffer-search-depth": 5
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
